{
    "name": "obadaalzidi/tap-php-sdk",
    "description": "A PHP SDK for Tap Payments",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Obadaalzidi\\TapPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "obada",
            "email": "opadaalziede@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "8.0.x-dev",
        "psr/http-client": "^1.0@dev",
        "psr/http-factory": "^1.1"
    },
    "require-dev": {
        "laravel/pint": "dev-main",
        "pestphp/pest": "4.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
